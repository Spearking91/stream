{
  "expo": {
    "name": "stream",
    "slug": "stream",
    "icon": "./assets/splash/foreground.png",
    "version": "1.0.1",
    "orientation": "portrait",
    "scheme": "stream",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.stream",
      "associatedDomains": [
        "applinks:stream-chat-app-mobile.firebaseapp.com"
      ]
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "stream-chat-app-mobile.firebaseapp.com"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "package": "com.anonymous.stream"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash/foreground.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#142632ff"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "80d64eb1-0228-43ab-a5bf-bb5baf6299ca"
      }
    }
  }
}
